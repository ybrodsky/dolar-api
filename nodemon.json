{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/*"
  ],
  "verbose": true,
  "execMap": {
    "js": "node --harmony"
  },
  "events": {
    "restart": "osascript -e 'display notification \"App restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
  },
  "watch": [
    "models/*",
    "views/*",
    "config/*",
    "lib/*",
    "index.js"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json"
}